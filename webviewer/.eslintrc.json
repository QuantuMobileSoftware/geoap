{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "plugins": ["react"],
  "rules": {
    "prettier/prettier": ["error", { "endOfLine": "auto" }],
    "react/prop-types": "off",
    "react/display-name": "off",
    "no-prototype-builtins": "off",
    "no-unused-vars": "warn",
    "no-unreachable": "warn"
  }
}
